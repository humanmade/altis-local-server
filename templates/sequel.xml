<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>ContentFilters</key>
		<dict/>
		<key>auto_connect</key>
		<true/>
		<key>data</key>
		<dict>
			<key>connection</key>
			<dict>
				<key>database</key>
				<string><%= MYSQL_DATABASE %></string>
				<key>host</key>
				<string><%= HOST %></string>
				<key>name</key>
				<string><%= MYSQL_USER %>@<%= HOST %></string>
				<key>password</key>
				<string><%= MYSQL_PASSWORD %></string>
				<key>port</key>
				<integer><%= PORT %></integer>
				<key>rdbms_type</key>
				<string>mysql</string>
				<key>ssh_host</key>
				<string></string>
				<key>ssh_user</key>
				<string></string>
				<key>ssh_port</key>
				<string></string>
				<key>ssh_keyLocation</key>
				<string></string>
				<key>ssh_keyLocationEnabled</key>
				<integer>0</integer>
				<key>sslCACertFileLocation</key>
				<string></string>
				<key>sslCACertFileLocationEnabled</key>
				<integer>0</integer>
				<key>sslCertificateFileLocation</key>
				<string></string>
				<key>sslCertificateFileLocationEnabled</key>
				<integer>0</integer>
				<key>sslKeyFileLocation</key>
				<string></string>
				<key>sslKeyFileLocationEnabled</key>
				<integer>0</integer>
				<key>type</key>
				<string>StandardConnection</string>
				<key>useSSL</key>
				<integer>0</integer>
				<key>user</key>
				<string><%= MYSQL_USER %></string>
			</dict>
			<key>session</key>
			<dict/>
		</dict>
		<key>encrypted</key>
		<false/>
		<key>format</key>
		<string>connection</string>
		<key>queryFavorites</key>
		<array/>
		<key>queryHistory</key>
		<array/>
		<key>rdbms_type</key>
		<string>mysql</string>
		<key>rdbms_version</key>
		<string><%= MYSQL_MAJOR %></string>
		<key>version</key>
		<integer>1</integer>
	</dict>
</plist>
